        squad_id = uuid4()
        
        # Mock responses for multiple users trying to join same squad
        mock_api_client.get.side_effect = [
            # All users not in squads initially
            AsyncMock(status_code=404),
            # Squad details for each join attempt
            AsyncMock(
                status_code=200,
                json=lambda: {
