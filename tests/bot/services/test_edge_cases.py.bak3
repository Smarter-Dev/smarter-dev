class MockUser:
    """Mock Discord user for testing transfer_bytes method."""
    
    def __init__(self, user_id: str, username: str):
        self._id = user_id
        self._username = username
    
    @property
    def id(self) -> str:
        return self._id
    
    def __str__(self) -> str:
        return self._username


