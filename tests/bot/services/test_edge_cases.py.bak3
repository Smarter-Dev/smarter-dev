            )
        ] * 10  # Repeat for multiple users
        
        # Mock successful joins (some may fail due to capacity)
        mock_api_client.post.return_value = AsyncMock(
            status_code=200,
            json=lambda: {"success": True}
        )
        
        # Multiple users trying to join concurrently
        tasks = []
